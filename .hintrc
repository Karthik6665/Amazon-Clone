{
  "extends": [
    "development"
  ],
  "hints": {
    "compat-api/html": [
      "default",
      {
        "ignore": [
          "meta[name=theme-color]"
        ]
      }
    ],
    "axe/structure": [
      "default",
      {
        "list": "off"
      }
    ],
    "axe/parsing": [
      "default",
      {
        "marquee": "off"
      }
    ],
    "compat-api/css": [
      "default",
      {
        "ignore": [
          "width: fit-content"
        ]
      }
    ],
    "meta-viewport": "off",
    "axe/language": [
      "default",
      {
        "html-lang-valid": "off"
      }
    ],
    "manifest-file-extension": "off",
    "apple-touch-icons": "off"
  }
}